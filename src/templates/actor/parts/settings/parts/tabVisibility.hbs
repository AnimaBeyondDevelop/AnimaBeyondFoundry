{{#> "systems/animabf/templates/common/ui/group.hbs"
  class="tab-visibility"
}}
  {{>
  "systems/animabf/templates/common/ui/group-header.hbs"
    title=(localize "anima.ui.settings.tabVisibility.title")
  }}
  {{#> "systems/animabf/templates/common/ui/group-body.hbs"
    class="tab-visibility"
  }}
    {{>
    "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
      class="mystic"
      title=(localize "anima.ui.settings.tabVisibility.mystic.title")
      inputType="checkbox"
      inputName="system.ui.tabVisibility.mystic.value"
      inputValue=system.ui.tabVisibility.mystic.value
    }}
    {{>
    "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
      class="domine"
      title=(localize "anima.ui.settings.tabVisibility.domine.title")
      inputType="checkbox"
      inputName="system.ui.tabVisibility.domine.value"
      inputValue=system.ui.tabVisibility.domine.value
    }}
    {{>
    "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
      class="psychic"
      title=(localize "anima.ui.settings.tabVisibility.psychic.title")
      inputType="checkbox"
      inputName="system.ui.tabVisibility.psychic.value"
      inputValue=system.ui.tabVisibility.psychic.value
    }}
    {{#if (settingBool "DEVELOP_MODE")}}
      {{>
      "systems/animabf/templates/common/ui/horizontal-titled-input.hbs"
        class="psychic"
        title=(localize "anima.ui.settings.tabVisibility.effects.title")
        inputType="checkbox"
        inputName="system.ui.tabVisibility.effects.value"
        inputValue=system.ui.tabVisibility.effects.value
      }}
    {{/if}}
  {{/"systems/animabf/templates/common/ui/group-body.hbs"}}
{{/"systems/animabf/templates/common/ui/group.hbs"}}
